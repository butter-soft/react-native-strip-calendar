{
  "name": "react-native-strip-calendar",
  "version": "0.1.2",
  "description": "react native strip calendar",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch ./src",
    "example:install": "cd example && pnpm install && cd ..",
    "example:prebuild": "cd example && pnpm prebuild && cd ..",
    "example:reset": "cd example && pnpm reset && cd ..",
    "example:android": "cd example && pnpm android && cd ..",
    "example:android-device": "cd example && pnpm android:device && cd ..",
    "example:ios": "cd example && pnpm ios && cd ..",
    "example:ios-device": "cd example && pnpm ios:device && cd .."
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "swarvy",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.0.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-native": "^0.78.0",
    "react-native-web": "^0.20.0"
  },
  "dependencies": {
    "@legendapp/list": "^2.0.14",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "tailwind-merge": "^3.3.1"
  }
}
